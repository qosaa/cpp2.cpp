#include <iostream>
using namespace std;

int main() {
    // Média de notas
    float notas[5];
    float soma = 0;
    float media;
    
    for (int i = 0; i < 5; i++) {
        cout << "Nota do aluno " << i + 1 << ": ";
        cin >> notas[i];
        soma += notas[i];
    }
    
    media = soma / 5;
    
    cout << "A média: " << media << endl;
    
    float maiorNota = notas[0];
    float menorNota = notas[0];
    
    for (int i = 1; i < 5; i++) {
        if (notas[i] > maiorNota) {
            maiorNota = notas[i];
        }
        if (notas[i] < menorNota) {
            menorNota = notas[i];
        }
    }
    
    cout << "maior nota: " << maiorNota << endl;
    cout << "menor nota: " << menorNota << endl;
    
    return 0;
}
